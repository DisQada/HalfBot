{
  "displayName": "HalfBot",
  "name": "@disqada/halfbot",
  "version": "3.1.3",
  "description": "A tool to facilitate and accelerate the development of Discord bots",
  "scripts": {
    "types": "workspace ./workspace.json -t",
    "docs": "workspace ./workspace.json -t -d",
    "format": "prettier . --write --ignore-path .gitignore",
    "test": "mocha ./test/**/*.test.js"
  },
  "type": "module",
  "main": "./src/exports.js",
  "types": "./types/exports.d.ts",
  "exports": {
    ".": {
      "import": "./src/exports.js",
      "default": "./src/exports.js",
      "types": "./types/exports.d.ts"
    }
  },
  "files": [
    "src",
    "types",
    "LICENSE",
    "*.md"
  ],
  "homepage": "https://disqada.github.io/HalfBot",
  "repository": {
    "type": "git",
    "url": "https://github.com/DisQada/HalfBot.git"
  },
  "bugs": {
    "url": "https://github.com/DisQada/HalfBot/issues"
  },
  "keywords": [
    "discord.js",
    "discord",
    "half",
    "bot",
    "client"
  ],
  "author": "DisQada <hi@disqada.org> (https://links.disqada.org)",
  "contributors": [],
  "license": "Apache-2.0",
  "dependencies": {
    "@disqada/pathfinder": "^2.0.3",
    "discord.js": "^14.10.2",
    "table": "^6.8.1"
  },
  "devDependencies": {
    "@disqada/workspace": "^1.5.2",
    "mocha": "^10.7.3",
    "prettier": "^3.3.3"
  }
}
