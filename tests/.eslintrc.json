{
    "root": true,
    "env": {
        "jest/globals": true,
        "es6": false,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:jest/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["jest"],
    "ignorePatterns": [],
    "rules": {
        "jest/no-conditional-expect": "off"
    }
}
